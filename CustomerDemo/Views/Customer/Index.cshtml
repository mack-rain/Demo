@model IEnumerable<CustomerDemo.CustomerModel>

<table class="table">
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Score</th>
            <th>Rank</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.CustomerID</td>
            <td>@item.Score</td>
            <td>@item.Rank</td>
        </tr>
        }
    </tbody>
</table>